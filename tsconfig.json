{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    /* Path alias */
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@ui/*": [
        "./src/components/ui/*"
      ],
      "@components/*": [
        "./src/components/*",
      ],
      "@hooks/*": [
        "./src/hooks/*",
      ],
      "@pages/*": [
        "./src/pages/*",
      ],
      "@context/*": [
        "./src/context/*",
      ],
      "@utils/*": [
        "./src/utils/*",
      ]
    },
  },
}